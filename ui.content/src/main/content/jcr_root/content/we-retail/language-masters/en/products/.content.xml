<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:catalogBlueprint="/content/catalogs/we-retail/en/website-catalog"
        cq:cloudserviceconfigs="[[]]"
        cq:commerceProvider="we-retail"
        cq:commerceType="section"
        cq:lastModified="{Date}2016-04-12T01:21:11.281+02:00"
        cq:lastModifiedBy="admin"
        cq:lastPropertyRollout="{Date}2016-04-12T01:19:37.591+02:00"
        cq:propertyInheritanceCancelled="[hideInNav]"
        cq:template="/conf/we-retail/settings/wcm/templates/section-page"
        jcr:language="en"
        jcr:mixinTypes="[cq:LiveSync,cq:PropertyLiveSyncCancelled]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Products"
        sling:resourceType="we-retail/components/structure/page"
        coverImage="/content/dam/we-retail/en/products/Product_catalog.jpg"
        hideInNav="true">
        <root
            jcr:mixinTypes="[cq:LiveRelationship]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <product-grid
                jcr:mixinTypes="[cq:LiveRelationship]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="we-retail/components/content/product-grid"
                displayAs="products"
                listFrom="descendants"
                orderBy="jcr:title"
                ordered="{Boolean}true"
                pageMax="0">
                <cq:responsive
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured">
                    <default
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        width="12"/>
                    <phone
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        width="12"/>
                </cq:responsive>
            </product-grid>
            <responsivegrid
                jcr:mixinTypes="[cq:LiveRelationship]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
        </root>
        <cq:LiveSyncConfig
            cq:isDeep="{Boolean}false"
            cq:master="/content/catalogs/we-retail/en/website-template-pages/catalog-page-template"
            cq:rolloutConfigs="[/etc/msm/rolloutconfigs/catalogpagecontent,/etc/msm/rolloutconfigs/catalogrollouthooks]"
            jcr:primaryType="cq:LiveCopy"/>
    </jcr:content>
    <women/>
    <men/>
    <equipment/>
</jcr:root>
