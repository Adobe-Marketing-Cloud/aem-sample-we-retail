<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:catalogBlueprint="/content/catalogs/we-retail/en/base-catalog/equipment/running"
        cq:commerceType="section"
        cq:lastModified="{Date}2016-02-25T22:18:54.181+01:00"
        cq:lastModifiedBy="admin"
        cq:lastPropertyRollout="{Date}2016-02-25T22:18:54.180+01:00"
        cq:template="/conf/we-retail/settings/wcm/templates/product-page"
        jcr:mixinTypes="[cq:LiveSync]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Running"
        sling:resourceType="we-retail/components/structure/page">
        <root
            jcr:mixinTypes="[cq:LiveRelationship]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:mixinTypes="[cq:LiveRelationship]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <responsivegrid
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <cq:responsive
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured">
                        <default
                            jcr:mixinTypes="[cq:LiveRelationship]"
                            jcr:primaryType="nt:unstructured"
                            width="2"/>
                    </cq:responsive>
                    <product_filter0
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="we-retail/components/content/product-filter"
                        type="toggle"/>
                    <product_filter1
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="we-retail/components/content/product-filter"
                        type="color"/>
                    <product_filter2
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="we-retail/components/content/product-filter"
                        type="price"/>
                </responsivegrid>
                <section_title
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="we-retail/components/content/title">
                    <cq:responsive
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured">
                        <default
                            jcr:mixinTypes="[cq:LiveRelationship]"
                            jcr:primaryType="nt:unstructured"
                            width="8"/>
                    </cq:responsive>
                </section_title>
                <product_filter
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="we-retail/components/content/product-filter"
                    type="sort">
                    <cq:responsive
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured">
                        <default
                            jcr:mixinTypes="[cq:LiveRelationship]"
                            jcr:primaryType="nt:unstructured"
                            width="2"/>
                    </cq:responsive>
                </product_filter>
                <product-grid
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="we-retail/components/content/product-grid"
                    displayAs="products"
                    listFrom="children"
                    orderBy="jcr:title"
                    pageMax="0">
                    <cq:responsive
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured">
                        <default
                            jcr:mixinTypes="[cq:LiveRelationship]"
                            jcr:primaryType="nt:unstructured"
                            width="10"/>
                    </cq:responsive>
                </product-grid>
            </responsivegrid>
        </root>
        <cq:responsive
            jcr:mixinTypes="[cq:LiveRelationship]"
            jcr:primaryType="nt:unstructured">
            <breakpoints
                jcr:mixinTypes="[cq:LiveRelationship]"
                jcr:primaryType="nt:unstructured">
                <phone
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured"
                    title="Phone"
                    width="{Decimal}650"/>
                <tablet
                    jcr:mixinTypes="[cq:LiveRelationship]"
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Decimal}1200"/>
            </breakpoints>
        </cq:responsive>
        <cq:LiveSyncConfig
            cq:isDeep="{Boolean}false"
            cq:master="/content/catalogs/we-retail/en/template-pages/section-page-template"
            cq:rolloutConfigs="[/etc/msm/rolloutconfigs/catalogpagecontent,/etc/msm/rolloutconfigs/catalogrollouthooks]"
            jcr:primaryType="cq:LiveCopy"/>
    </jcr:content>
